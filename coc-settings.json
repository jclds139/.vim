{
	"languageserver": {
		"ccls": {
			"command": "ccls",
				"filetypes": [
				"c",
				"cpp",
				"objc",
				"objcpp"
				],
				"rootPatterns": [
					".ccls",
				"compile_commands.json",
				".vim/",
				".git/",
				".hg/"
				],
				"initializationOptions": {
					"cache": {
						"directory": "/tmp/ccls"
					}
				}
		}
	},
	"java.home" : "/usr/lib/jvm/default/",
	"diagnostic-languageserver.filetypes": {
		"tiddlywiki": [ "write-good","languagetool"],
		"tex": [ "write-good","languagetool"],
		"latex": [ "write-good","languagetool"]
	}
}
